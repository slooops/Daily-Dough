# Plaid Configuration (Add your actual Plaid credentials)
PLAID_CLIENT_ID=your_plaid_client_id_here
PLAID_SECRET=your_plaid_secret_here
PLAID_ENV=sandbox

# Database Configuration (Provided by Vercel CLI via `vercel env pull .env.development.local`)
DATABASE_URL=your_database_url_here
DATABASE_URL_UNPOOLED=your_database_url_unpooled_here

# Postgres-specific variables (also from Vercel)
POSTGRES_URL=your_postgres_url_here
POSTGRES_URL_NON_POOLING=your_postgres_url_non_pooling_here
POSTGRES_URL_NO_SSL=your_postgres_url_no_ssl_here
POSTGRES_PRISMA_URL=your_postgres_prisma_url_here
POSTGRES_USER=your_postgres_user_here
POSTGRES_HOST=your_postgres_host_here
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DATABASE=your_postgres_database_here

# Individual connection parameters
PGDATABASE=your_pg_database_here
PGHOST=your_pg_host_here
PGHOST_UNPOOLED=your_pg_host_unpooled_here
PGPASSWORD=your_pg_password_here
PGUSER=your_pg_user_here

# Encryption Key (32 bytes base64 encoded)
# Generate with: openssl rand -base64 32
ENCRYPTION_KEY=your_32_byte_base64_encryption_key_here

# Neon-specific variables (auto-generated by Vercel)
NEON_PROJECT_ID=your_neon_project_id_here

# Vercel Stack variables (if using Vercel's auth features)
NEXT_PUBLIC_STACK_PROJECT_ID=your_stack_project_id_here
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=your_stack_publishable_key_here
STACK_SECRET_SERVER_KEY=your_stack_secret_key_here

# Vercel OIDC Token (auto-generated, rotates)
VERCEL_OIDC_TOKEN=your_vercel_oidc_token_here
